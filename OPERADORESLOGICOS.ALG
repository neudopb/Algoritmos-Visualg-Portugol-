Algoritmo "operadoresLogicos"

Var
   A, B, C : Inteiro

Inicio

      A <- 2
      B <- 3
      C <- 5

      Escreval (verdadeiro e falso)
      Escreval (verdadeiro ou falso)
      
      Escreval ((A=B) ou (C>A))
      Escreval (nao((A=B) ou (C>A)))

Fimalgoritmo

