Algoritmo "conversor"

Var
   reais, dolares : real

Inicio

      Escreva ("Quantos reais eu tenho?")
      Leia (reais)
      dolares <- reais / 5.32
      Escreva ("Posso ter US $", dolares)

Fimalgoritmo

