Algoritmo "repita1a10"

Var
   cont, n : inteiro

Inicio

   cont <- 1
   Escreva("Numero Tabuada: ")
   Leia(n)
   
   Repita
      EscrevaL(n, " x ", cont, " = ", n * cont)
      cont <- cont + 1
   Ate (cont > 10)

Fimalgoritmo

